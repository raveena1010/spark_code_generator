from pyspark.sql import SparkSession
spark = SparkSession.builder.appName("SparkApp").master("local[5]").getOrCreate()
from pyspark.sql.functions import col
from pyspark.sql.functions import expr
StockPrice = spark.read.option('header',True).option('delimiter',',').option('inferSchema',True).csv('loc')
StockPrice = StockPrice.alias("df").select(col('date').alias('dates'),col('date').alias('ds'),'symbol',)
StockPrice = StockPrice.select(['dates', 'ds', 'symbol'])
