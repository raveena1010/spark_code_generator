Stockcompanies = spark.read.option('header',True).option('delimiter',',').option('inferSchema',True).csv('loc')
Stockcompanies_b0= Stockcompanies
Stockcompanies_b1= Stockcompanies
Stockcompanies_b2= Stockcompanies
Salaries = spark.read.option('header',True).option('delimiter',',').option('inferSchema',True).csv('loc1')
Salaries_b0= Salaries
Salaries_b1= Salaries
Stockcompanies_b0 = Stockcompanies_b0.filter('Ticker_symbol between "ABT" and "APD"')
Stockcompanies_b1 = Stockcompanies_b1.select(['Sector', 'Ticker_symbol', 'Security'])
Stockcompanies_b2 = Stockcompanies_b2.filter('Sector in ("Health Care","Information Technology")')
Stockcompanies_b2_b0= Stockcompanies_b2
Stockcompanies_b2_b1= Stockcompanies_b2
Stockcompanies_b0 = Stockcompanies_b0.select(['Sub_Industry', 'Ticker_symbol', 'Sector', 'Security'])
Stockcompanies_b2_b0 = Stockcompanies_b2_b0.select(['Ticker_symbol'])
Stockcompanies_b2_b1 = Stockcompanies_b2_b1.select(['Headquarter', 'Ticker_symbol', 'Sub_Industry', 'Security', 'Sector'])
Stockcompanies_b0 = Stockcompanies_b0.select(['Ticker_symbol', 'Sector', 'Security'])
Stockcompanies_b2_b0 = Stockcompanies_b2_b0.filter('Ticker_symbol not in ("ABT","ABBV","ADBE")')
Stockcompanies_b2_b1 = Stockcompanies_b2_b1.select(['Headquarter'])
Stockcompanies_b2_b1 = Stockcompanies_b2_b1.filter('Headquarter in ("North Chicago; Illinois")')
Salaries_b0 = Salaries_b0.select(['BasePay', 'Year', 'Benefits', 'OvertimePay', 'TotalPayBenefits', 'JobTitle', 'Agency', 'TotalPay', 'Id', 'EmployeeName', 'OtherPay'])
Salaries_b1 = Salaries_b1.filter(' TotalPay between 500000 and 800000')
Salaries_b0 = Salaries_b0.filter('JobTitle not in ("CAPTAIN III (POLICE DEPARTMENT)")')
